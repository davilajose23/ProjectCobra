main
    int a[10], aux, index
    aux = 10
    for i from 0 to 10:
        a[i] = aux
        aux -= 1
    end
    index = -1
    aux = 3
    for i from 0 to 10:
        if a[i] == aux:
            index = i
        end
    end

    if index == -1:
        print 'Value not found'
    else:
        print 'Index'
        print index
    end
endmain